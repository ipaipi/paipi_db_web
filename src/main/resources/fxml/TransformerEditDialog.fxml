<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.paipi.controller.TransformerEditDialogController" spacing="12"
      style="-fx-padding: 20;">
    <Label text="编辑Transformer参数" style="-fx-font-weight: bold; -fx-font-size: 15;"/>
    <HBox spacing="8">
        <Label text="类型："/>
        <ComboBox fx:id="typeComboBox" prefWidth="180"/>
    </HBox>
    <GridPane fx:id="paramGrid" hgap="10" vgap="8"/>
    <HBox spacing="10" alignment="CENTER_RIGHT">
        <Button text="确定" onAction="#onOk"/>
        <Button text="取消" onAction="#onCancel"/>
    </HBox>
</VBox> 